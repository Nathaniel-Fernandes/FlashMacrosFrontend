{
  "expo": {
    "name": "FlashMacros",
    "slug": "FlashMacros",
    "scheme": "myappdafadf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nathanielfernandes.FlashMacros"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Flash Macros to access your camera."
        }
      ],
      ["react-native-vision-camera",
      {
        "cameraPermission": "Allow Flash Macros to access your camera.",
        "enableMicrophonePermission": false
      }]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e30cc337-803f-4d28-af23-7d23b3d8eb6e"
      }
    }
  }
}

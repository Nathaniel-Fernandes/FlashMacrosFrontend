{
  "expo": {
    "name": "FlashMacros",
    "slug": "FlashMacros",
    "scheme": "myappdafadf",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nathanielfernandes.FlashMacros",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Flash Macros to access your camera."
        }
      ],
      ["react-native-vision-camera",
      {
        "cameraPermission": "Allow Flash Macros to access your camera.",
        "enableMicrophonePermission": false
      }],
      "@react-native-google-signin/google-signin",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow FlashMacros to access your photos.",
          "savePhotosPermission": "Allow FlashMacros to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "appID": "316702201220506",
          "displayName": "FlashMacros",
          "clientToken": "bf21f6ff7aaf8c71a1a39dbce3ac0b20",
          "scheme": "fb316702201220506"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e30cc337-803f-4d28-af23-7d23b3d8eb6e"
      }
    }
  }
}
